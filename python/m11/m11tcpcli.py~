#!/usr/bin/python
import socket
target_host = "itss.biomea.com"
target_port = 80

client = socket.socket(socket.AF_INET,socket.SOCK_STREAM)
 # connect the client

client.connect((target_host,target_port))
# send some data

client.send("GET / HTTP/1.1\r\nHost: itss.biomea.com/itsc203/cgi-bin./echo.cgi\r\n\r\n")
 
# receive some data
response = client.recv(4096)
print response
