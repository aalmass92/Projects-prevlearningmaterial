#!/usr/bin/python

import sys, random

typeofbit = sys.argv[1]

if typeofbit == 'b':
    print random.getrandbits(8) 

elif typeofbit == 'w':
    print random.getrandbits(16)

elif typeofbit == 'd':
    print random.getrandbits(32)

elif typeofbit == 'q':
    print random.getrandbits(64)


#print random.getrandbits(8)
